<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="content">
            <block class="Ppsoft\CustomCheckout\Block\Onepage\Success" name="sidebar.success" template="Ppsoft_CustomCheckout::sidebar.phtml" after="-"/>
        </referenceBlock>
        <referenceBlock name="page.main.title" remove="true" />
        <referenceBlock name="checkout.registration" remove="true" />
        <referenceBlock name="global_messages" remove="true" />
        <referenceBlock name="messages" remove="true" />
    </body>
</page>
