<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="category.image" remove="true"/>
        <move element="page.main.title" destination="content" before="-"/>
        <referenceBlock name="product_list_impression" remove="true" />
        <referenceBlock name="category.description" remove="true" />
        <referenceBlock name="sidebar.additional" remove="false"/>
        <referenceBlock name="multiple-wishlist_sidebar" remove="true"/>
        <referenceBlock name="sale.reorder.sidebar" remove="true"/>
    </body>
</page>
